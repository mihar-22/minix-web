{
  "name": "minix/minix",
  "description": "The Minix Web Application.",
  "require": {
    "php": ">=7.0.0",
    "fideloper/proxy": "~3.3",
    "laravel/framework": "5.5.*",
    "laravel/passport": "^4.0",
    "laravel/tinker": "~1.0",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "doctrine/dbal": "^2.6",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "app/Modules/Auth/database/seeds",
      "app/Modules/Exchange/database/seeds"
    ],
    "psr-4": {
      "Minix\\": "app/",
      "Minix\\Auth\\": "app/Modules/Auth/src",
      "Minix\\Uuid\\": "app/Modules/Uuid/src",
      "Minix\\Exchange\\": "app/Modules/Exchange/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Minix\\Auth\\Tests\\": "app/Modules/Auth/tests/",
      "Minix\\Exchange\\Tests\\": "app/Modules/Exchange/tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
